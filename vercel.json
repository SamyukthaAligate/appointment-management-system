{
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth.js"
    },
    {
      "source": "/api/doctors",
      "destination": "/api/doctors.js"
    },
    {
      "source": "/api/appointments/(.*)",
      "destination": "/api/appointments.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
