{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "functions": {
    "api/auth/signup.js": {
      "runtime": "nodejs18.x"
    },
    "api/auth/login.js": {
      "runtime": "nodejs18.x"
    },
    "api/doctors.js": {
      "runtime": "nodejs18.x"
    },
    "api/appointments.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/auth/signup",
      "destination": "/api/auth/signup.js"
    },
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login.js"
    },
    {
      "source": "/api/doctors",
      "destination": "/api/doctors.js"
    },
    {
      "source": "/api/appointments/(.*)",
      "destination": "/api/appointments.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
